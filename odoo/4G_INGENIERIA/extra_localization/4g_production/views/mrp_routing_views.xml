<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data >
        <record id="mrp_routing_workcenter_tree_view_inherit_4g_production" model="ir.ui.view">
            <field name="name">mrp.routing.workcenter.view.form.inherit</field>
            <field name="model">mrp.routing.workcenter</field>
            <field name="inherit_id" ref="mrp.mrp_routing_workcenter_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='time_cycle']" position="after">
                    <field name="duracion_estimada" widget="float_time"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>